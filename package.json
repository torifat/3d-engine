    "husky": "^4.2.1",
    "parcel": "^1.12.4",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">= 12.15.0"
  }
